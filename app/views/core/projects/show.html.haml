- content_for :title do
  #{@core_project.name} Â· #{@account.username}
.panel.panel-default.sheet{"data-intro" => "See all your data sets in your current project here", "data-position" => "bottom"}
  .panel-body
    = link_to "Reports", account_project_impl_reports_path(@account, @core_project), style: "border-bottom: dotted 1px gray"
    &nbsp;
    |
    &nbsp;
    = link_to "Blacklist Datasets", account_project_impl_blacklist_datasets_path(@account, @core_project), style: "border-bottom: dotted 1px gray"
    %hr{style: "margin-bottom: 5px; margin-top: 5px; border-top-color: lightgray;"}
    - if 1 == 2
      %hr{style: "margin-bottom: 5px; border-top-color: lightgray;"}
      = render partial: "core/datacasts/index", locals: {sudo: @sudo, core_project: @core_project, account: @account, impl_aggregation: @impl_aggregation, core_datacasts: @core_datacasts}
      %hr{style: "margin-top: 5px; border-top-color: lightgray;"}
      .row
        .col-sm-12
          -if @pending_datacast_pulls.present? and current_account.present? and @sudo[1]
            %hr
            = render partial: "core/datacast_pulls/index", locals: {pending_datacast_pulls: @pending_datacast_pulls, sudo_1: @sudo[1]}
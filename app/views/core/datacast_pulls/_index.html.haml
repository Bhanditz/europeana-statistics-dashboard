.well
  %h4 Data Sets being uploaded from WEB Addresses (URLs)
  %hr
  %table.table.table-condensed
    %thead
      %th URL
      %th Status
      %th Date Modified
      %th
    %tbody
    -@pending_datacast_pulls.each do |datacast_pull|
      %tr
        %td
          =link_to datacast_pull.file_url, edit_account_core_project_datacast_pull_path(datacast_pull.core_project.account,datacast_pull.core_project,datacast_pull)
        %td= datacast_pull.status.html_safe
        %td.hint= time_ago_in_words(datacast_pull.updated_at)
        -if @sudo[1]
          %td
            = icon_del(account_core_project_datacast_pull_path(datacast_pull.core_project.account,datacast_pull.core_project,datacast_pull))